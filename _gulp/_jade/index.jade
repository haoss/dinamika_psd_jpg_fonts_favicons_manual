doctype

html(lang="ru")
  
  -var active = 'Главная' //активный пункт меню, список в header.jade
  -var pageTitle = 'Главная' + ' - Dinamika' //title страницы для генерации верстки  

  include ./_include/_head

  body
    
    // Preloader
    include ./_include/_preloader
    
    include ./_include/_header
    
    // Content
    .content
      
      // Car list
      .car-list
        div
          .car-list__block.car-list__block--big
            .car-list__title
              .car-list__title__top 
                strong Аренда
                span с водителем и без
                a(href="#")
              .car-list__title__bottom 
                ul
                  li Сhevrolet Camaro
                  li AUDI a6
                  li BMW 520
                  li Mercesrs E 200
                  li Porsche Cayenne
                  li и другие..
              
            img(src="./img/car-list/car-1/car-list-1.png", alt="").car-list__auto
            img(src="./img/car-list/car-1/car-human-1.png", alt="").car-list__human
            img(src="./img/car-list/car-1/car-human-1-smoke.png", alt="").car-list__smoke
        
        div
          .car-list__block
            .car-list__title
              .car-list__title__top 
                strong такси
                span премиум класс
                a(href="#")
            img(src="./img/car-list/car-2/car-list-2.png", alt="").car-list__auto
            img(src="./img/car-list/car-2/car-human-2.png", alt="").car-list__human
            
          .car-list__block
            .car-list__title
              .car-list__title__top 
                strong VIP Перевозки
                span встреча в делегации
                a(href="#")
            img(src="./img/car-list/car-3/car-list-3.png", alt="").car-list__auto
            img(src="./img/car-list/car-3/car-human-3.png", alt="").car-list__human
      
          .car-list__block
            .car-list__title
              .car-list__title__top 
                strong Сопровождение
                span c охраной
                a(href="#")
            img(src="./img/car-list/car-4/car-list-4.png", alt="").car-list__auto
            img(src="./img/car-list/car-4/car-human-4.png", alt="").car-list__human
      
          .car-list__block
            .car-list__title
              .car-list__title__top 
                strong VIP Мероприятия
                span свадьба, банкеты и т.д.
                a(href="#")
            img(src="./img/car-list/car-5/car-list-5.png", alt="").car-list__auto
            img(src="./img/car-list/car-5/car-human-5.png", alt="").car-list__human
      
      //- div(style='height: 200px')
        
      // Order row
      .order-row
        .container
          
          .order-row__top
            .row
              .col-xs-9.col-sm-6
                .order-row__title Заказать автомобиль
              .col-xs-3.col-sm-6
                a(href="#").order-row__button
                  em.hidden-xs Развернуть фильтр
                  span.order-row__radio
              
          #order-row__bottom.order-row__bottom
            .order-row__form
              
              form(action="#")
                .row
                  .col-md-6
                    .order-row__select
                      select.styler
                        option(value="" selected) Выберите автомобиль
                        option(value="BMW 520") BMW 520
                        option(value="mercedes e 200") mercedes e 200
                        option(value="audi a 6") audi a 6
                        option(value="chevrolet camaro") chevrolet camaro
                        option(value="toyota land cruiser") toyota land cruiser
                        option(value="Porsche cayenne") Porsche cayenne
                      
                    .order-row__img
                      .order-row__bottom-title BMW 520
                      img(src="./tmp/cars/cars-1.png", alt="").img-responsive
                    
                  .col-md-6.col-order-right
                    
                    .row
                      .col-480-6.col-sm-6
                        label.label-control.label-control--big Представьтесь
                        .form-group
                          input(type="text" placeholder="Введите ФИО").form-control.form-control--light
                          
                      .col-480-6.col-sm-6
                        label.label-control.label-control--big Телефон
                        .form-group
                          input(type="text" placeholder="Введите актуальный телефон").form-control.form-control--light                          
                    
                    label.label-control.label-control--auto Выберите даты аренды
                    
                    .row
                      .col-480-6.col-sm-6
                        .form-group.form-group--dark.form-check-required.form-group--has-icon
                          .required-info Обязательное поле!
                          input(type="text" placeholder="Выдача авто").form-control.form-control--light
                          i.ion-android-calendar
                          
                      .col-480-6.col-sm-6
                        .form-group.form-group--dark.form-check-required.form-group--has-icon
                          .required-info Обязательное поле!
                          input(type="text" placeholder="Возврат авто").form-control.form-control--light
                          i.ion-android-calendar
                            
                    .row
                      .col-480-6.col-sm-6
                        
                        .form-group
                          .row
                            .col-lg-6
                              label.label-control.label-control--auto.label-control--auto-lg Количество дней
                              
                            .col-lg-6
                              .form-control.form-control--light.form-control--quantity
                                span.color.color--red: strong 2 
                                | дня
                        
                      .col-480-6.col-sm-6
                        
                        .form-group
                          .row
                            .col-lg-5
                              label.label-control.label-control--auto.label-control--auto-lg Цена в сутки
                              
                            .col-lg-7
                              .form-control.form-control--light.form-control--quantity.big
                                span.color.color--red: strong 2399 
                                | руб
        
                    .row
                      .col-lg-6
                        .form-checkbox__row.form-checkbox__row--dark.no-margin.form-check-required
                          .required-info Обязательное поле!
                          label.form-checkbox.form-checkbox--white
                            input(type="checkbox")
                            span
                            span.span: em Выезд за пределы Московской области +15%
                        
                        .auto__price.auto__price--white Общая сумма:
                          strong 7699
                          span руб
                        
                      .col-lg-6
                        button(type="submit").btn.btn--big-order-left.button-check-required Заказать авто
      
